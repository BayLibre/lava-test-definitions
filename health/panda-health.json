{
    "timeout": 900,
    "device_type": "panda-es",
    "job_name": "panda-health",
    "logging_level": "DEBUG",
    "actions": [
        {
            "command": "deploy_linaro_kernel",
            "parameters": {
                "dtb": "file:///home/powerci/POWERCI/images/panda-es/omap4-panda-es.dtb",
                "kernel": "file:///home/powerci/POWERCI/images/panda-es/zImage",
		"overlays": ["file:///home/powerci/POWERCI/images/panda-es/modules.tar.xz"],
                "ramdisk": "http://storage.kernelci.org/images/rootfs/buildroot/armel/rootfs.cpio.gz"
            }
        },
        {
          "command": "boot_linaro_image"
        },
        {
            "command": "lava_test_shell",
            "parameters": {
                "testdef_repos": [
                    {
                        "git-repo": "git://git.linaro.org/qa/test-definitions.git",
                        "testdef": "openembedded/kernel-version.yaml"
                    }
                ],
                "timeout": 2400
            }
        },
        {
            "command": "submit_results_on_host",
            "parameters": {
                "server": "http://lava-baylibre.local:10080/RPC2/",
                "stream": "/anonymous/lab-health/"
            }
        }
    ]
}
